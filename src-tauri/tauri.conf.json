{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "Camunda BPMN Comparer",
  "version": "1.0.0",
  "identifier": "com.camunda-bpmn.comparer",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:3000",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "Camunda BPMN Comparer",
        "width": 1070,
        "height": 768,
        "minWidth": 1070,
        "minHeight": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}